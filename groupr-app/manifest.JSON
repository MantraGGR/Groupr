{
  "manifest_version": 3,
  "name": "AI Tab Organizer",
  "version": "1.0",
  "description": "Organize tabs into user-defined groups automatically",
  "permissions": ["tabs", "tabGroups", "storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
